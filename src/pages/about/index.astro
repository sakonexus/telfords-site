---
import MainLayout from '../../layouts/MainLayout.astro'
import AboutPage from '@components/AboutPage'
import { getImage } from 'astro:assets';
import telfordsMural from '@assets/telfords-mural.jpg'
import telfordsPic from '@assets/susan-and-brian-telford.jpg'

const telfordsMuralImage = await getImage({ src: telfordsMural });
const telfordsOwnersImage = await getImage({ src: telfordsPic });
---

<MainLayout>
	<AboutPage headerImg={telfordsMuralImage} ownersImg={telfordsOwnersImage} />
</MainLayout>